{
  "name": "sphinx-to-tr",
  "version": "0.0.1",
  "description": "transform Sphinx doc (e.g. <https://webassembly.github.io/spec/core/index.html>) to <http://www.w3.org/TR/>",
  "main": "SphinxToTr.js",
  "bin": [
    "bin/sphinx-to-tr"
  ],
  "scripts": {
    "wasm": "./bin/sphinx-to-tr ../../webassembly/spec/core/index.html wasm-respec.html out Appendix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3c/sphinx-to-tr.git"
  },
  "keywords": [
    "sphinx",
    "w3c",
    "specification"
  ],
  "author": {
    "name": "Eric Prud'hommeaux",
    "email": "eric@w3.org",
    "url": "http://www.w3.org/People/Eric/"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/w3c/sphinx-to-tr/issues"
  },
  "homepage": "https://github.com/w3c/sphinx-to-tr#readme",
  "dependencies": {
    "js-yaml": "^4.1.0",
    "jsdom": "^16.6.0",
    "respec": "^26.9.1"
  }
}
